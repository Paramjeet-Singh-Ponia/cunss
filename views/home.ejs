<!--Contact us Section-->
       <!--Contact US-->
       <section id="contact" class="contact " style="background: linear-gradient(#bcbec0 0% , #9cd8d1 50%, #b2c9cf 100%); height: 100vh; width: 100%;">
        <div class="container p-5 ">
            <h2 class="section-heading text-center m-3" style="font-style: italic; font-size: 50px;">Contact Us:</h2>
            <form class="col-lg-6 offset-lg-3 card-body bg-info">
                <div class="form-group">
                    <label for="email">Email address</label>
                    <input type="email" id="email" class="form-control" aria-describedby="emailHelp" placeholder="Enter email">
                  </div>
                <div class="form-group">
                    <label for="name">Name</label>
                    <input type="text" id="name" class="form-control" placeholder="Enter name">
                </div>
                <div class="form-group">
                    <label for="message">Message</label>
                    <textarea class="form-control" id="message" placeholder="Type your message" row="5" cols="10">
                    
                </textarea>
                </div>
                <div class="form-group form-check">
                    <input type="checkbox" class="form-check-input" id="check">
                    <label class="form-check-label" for="check">Subscribe to newsletter</label>
                </div>
                <div class="text-center">
                    <button class="btn btn-lg bg-dark text-white btn-color cont-btn">Submit</button>
                </div>
            </form>
        </div>
    </section>
    <!--End of Contact Section-->



